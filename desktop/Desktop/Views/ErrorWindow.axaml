<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        x:Class="Desktop.Views.ErrorWindow"
        Width="300" Height="150">
    <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center">
        <TextBlock Name="ErrorMessage" TextWrapping="Wrap" HorizontalAlignment="Center"/>
        <Button x:Name="OkButton" Content="OK" HorizontalAlignment="Center" Margin="0,10,0,0" Click="OkButton_Click"/>
    </StackPanel>
</Window>